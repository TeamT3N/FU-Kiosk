<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">


    <TextView
        android:id="@+id/text_dashboard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="111dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/dafa"
        android:layout_width="94dp"
        android:layout_height="115dp"
        android:background="@drawable/archi_college"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toTopOf="@id/chm"
        app:layout_constraintEnd_toStartOf="@id/cn"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toEndOf="@id/ca"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.857" />

    <Button
        android:id="@+id/cas"
        android:layout_width="75dp"
        android:layout_height="117dp"
        android:background="@drawable/cass"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toTopOf="@id/cn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.934"
        app:layout_constraintStart_toEndOf="@id/cc"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.808" />

    <Button
        android:id="@+id/cba"
        android:layout_width="84dp"
        android:layout_height="114dp"
        android:background="@drawable/cba_college"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toTopOf="@id/ca"
        app:layout_constraintEnd_toStartOf="@id/cc"
        app:layout_constraintHorizontal_bias="0.388"
        app:layout_constraintStart_toEndOf="@id/ccs"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.791" />

    <Button
        android:id="@+id/ccs"
        android:layout_width="75dp"
        android:layout_height="114dp"
        android:background="@drawable/ccs_college"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toTopOf="@id/sie"
        app:layout_constraintEnd_toStartOf="@id/cba"
        app:layout_constraintHorizontal_bias="0.692"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.791" />

    <Button
        android:id="@+id/chm"
        android:layout_width="88dp"
        android:layout_height="121dp"
        android:background="@drawable/hosm_college"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toEndOf="@id/ce"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.647" />

    <Button
        android:id="@+id/ca"
        android:layout_width="75dp"
        android:layout_height="117dp"
        android:background="@drawable/agri_college"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toTopOf="@id/ce"
        app:layout_constraintEnd_toStartOf="@id/dafa"
        app:layout_constraintHorizontal_bias="0.377"
        app:layout_constraintStart_toEndOf="@id/sie"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.863" />

    <Button
        android:id="@+id/cn"
        android:layout_width="75dp"
        android:layout_height="119dp"
        android:background="@drawable/nursing_college"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toTopOf="@id/chm"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.934"
        app:layout_constraintStart_toEndOf="@id/dafa"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.87" />

    <Button
        android:id="@+id/ce"
        android:layout_width="81dp"
        android:layout_height="121dp"
        android:background="@drawable/educ_college"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/chm"
        app:layout_constraintHorizontal_bias="0.844"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.647" />

    <Button
        android:id="@+id/cc"
        android:layout_width="73dp"
        android:layout_height="114dp"
        android:background="@drawable/crim_college"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toTopOf="@id/dafa"
        app:layout_constraintEnd_toStartOf="@id/cas"
        app:layout_constraintHorizontal_bias="0.665"
        app:layout_constraintStart_toEndOf="@id/cba"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.791" />

    <Button
        android:id="@+id/sie"
        android:layout_width="74dp"
        android:layout_height="117dp"
        android:background="@drawable/engineering_college"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toTopOf="@id/ce"
        app:layout_constraintEnd_toStartOf="@id/ca"
        app:layout_constraintHorizontal_bias="0.547"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.863" />


</androidx.constraintlayout.widget.ConstraintLayout>